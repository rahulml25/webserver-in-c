            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
            "args": [
                "-std=c99"
            ],
            "stopAtEntry": false,
            "cwd": "C:/Development/SDKs/mingw64/bin",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\Development\\SDKs\\mingw64\\bin\\gdb.exe",
